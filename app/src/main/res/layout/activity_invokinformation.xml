<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".invokinformation">

    <EditText
        android:id="@+id/invno"
        android:layout_width="288dp"
        android:layout_height="59dp"
        android:ems="10"
        android:inputType="text"
        android:hint="INV.NO"
        app:layout_constraintBottom_toTopOf="@+id/date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/date"
        android:layout_width="284dp"
        android:layout_height="63dp"
        android:ems="10"
        android:inputType="text"
        android:hint="DATE"
        app:layout_constraintBottom_toTopOf="@+id/retalernames"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invno" />

    <Spinner
        android:id="@+id/retalernames"
        android:layout_width="284dp"
        android:layout_height="63dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/amount"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/date" />

    <EditText
        android:id="@+id/amount"
        android:layout_width="288dp"
        android:layout_height="61dp"
        android:ems="10"
        android:inputType="text"
        android:hint="Amount"
        app:layout_constraintBottom_toTopOf="@+id/qyt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/retalernames" />

    <EditText
        android:id="@+id/qyt"
        android:layout_width="288dp"
        android:layout_height="61dp"
        android:ems="10"
        android:inputType="text"
        android:hint="QYT"
        app:layout_constraintBottom_toTopOf="@+id/uplodeinv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amount" />

    <Button
        android:id="@+id/uplodeinv"
        android:layout_width="263dp"
        android:layout_height="57dp"
        android:text="uplodecv"
        app:layout_constraintBottom_toTopOf="@+id/submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qyt" />

    <Button
        android:id="@+id/submit"
        android:layout_width="239dp"
        android:layout_height="53dp"
        android:text="submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uplodeinv" />
</androidx.constraintlayout.widget.ConstraintLayout>